{% extends 'base.html.twig' %}

{% block title %}Address{% endblock %}

{% block body %}
    <h1>Address</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ address.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ address.name }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ address.country }}</td>
            </tr>
            <tr>
                <th>Department</th>
                <td>{{ address.department }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ address.city }}</td>
            </tr>
            <tr>
                <th>Postal_code</th>
                <td>{{ address.postalCode }}</td>
            </tr>
            <tr>
                <th>Address_line_1</th>
                <td>{{ address.addressLine1 }}</td>
            </tr>
            <tr>
                <th>Address_line_2</th>
                <td>{{ address.addressLine2 }}</td>
            </tr>
            <tr>
                <th>Address_line_3</th>
                <td>{{ address.addressLine3 }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin.address.index') }}">back to list</a>

    <a href="{{ path('admin.address.edit', {'id': address.id}) }}">edit</a>

    {{ include('admin/address/_delete_form.html.twig') }}
{% endblock %}
